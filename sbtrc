alias trace = set traceLevel := 0
alias off = set offline := true
alias opt = set scalacOptions ++= Seq("-Xdisable-assertions", "-optimize", "-Yinline")
alias warn = set scalacOptions ++= Seq("-Xfatal-warnings")
alias nowarn = set scalacOptions --= Seq("-Xfatal-warnings")
alias lean = clean
alias cl = clean
alias cn = console
alias c = compile
alias cc = ~compile
alias l = nativeLink
alias ll = ~nativeLink
alias lll = clean;~nativeLink
alias r = run
alias rr = ~run
alias t = test
alias tt = ~test
alias tc = test:compile
alias tcc = ~test:compile
alias pl = publishLocal
alias ppl = ~publishLocal
alias du = dependencyUpdates
alias re = reload
alias rec = reload; compile
alias recc = reload; ~compile
alias up = update
alias err = set maxErrors := 
alias ls = projects
alias cd = project
alias coverageAll = clean ;coverage ;test ;coverageReport
